{
  "name": "@symbx/vue-fomantic-ui",
  "version": "0.14.0",
  "description": "Fomantic-UI component library for Vue.js 3.0 (fork of https://github.com/nightswinger/vue-fomantic-ui)",
  "license": "MIT",
  "bugs": "https://github.com/symbx/vue-fomantic-ui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/symbx/vue-fomantic-ui"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-fomantic-ui.umd.js",
  "module": "./dist/vue-fomantic-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-fomantic-ui.es.js",
      "require": "./dist/vue-fomantic-ui.umd.js"
    }
  },
  "keywords": [
    "semantic-ui",
    "fomantic-ui",
    "vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "test": "vitest"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-vue": "^1.1.1",
    "@vitejs/plugin-vue-jsx": "^1.0.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^8.8.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "happy-dom": "^2.31.1",
    "typescript": "^4.1.3",
    "vite": "^2.7.13",
    "vitest": "^0.2.5",
    "vue": "^3.0.5",
    "vue-tsc": "^0.34.2"
  }
}
